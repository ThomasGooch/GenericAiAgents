{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Agent": "Debug"
    }
  },
  "AllowedHosts": "*",
  "AI": {
    "Provider": "OpenAI",
    "ModelId": "gpt-4",
    "ApiKey": "your-openai-api-key-here",
    "MaxTokens": 2000,
    "Temperature": 0.7,
    "TopP": 1.0,
    "TimeoutSeconds": 30,
    "AdditionalSettings": {
      "user": "basicintegration-sample"
    }
  },
  "SecretManagement": {
    "Type": "Environment",
    "EnvironmentPrefix": "AGENT_SECRET_",
    "EnableCaching": true,
    "CacheExpirationMinutes": 30
  },
  "JWT": {
    "SigningKey": "development-key-minimum-32-characters-for-security",
    "Issuer": "basicintegration-sample",
    "Audience": "basicintegration-api"
  }
}